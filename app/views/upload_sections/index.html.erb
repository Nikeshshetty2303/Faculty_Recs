<p id="notice"><%= notice %></p>

<h1>Upload Sections</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @upload_sections.each do |upload_section| %>
      <tr>
        <td><%= upload_section.title %></td>
        <td><%= link_to 'Show', upload_section %></td>
        <td><%= link_to 'Edit', edit_upload_section_path(upload_section) %></td>
        <td><%= link_to 'Destroy', upload_section, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Upload Section', new_upload_section_path %>
