<link href="https://fonts.googleapis.com/css2?family=Gabarito&display=swap" rel="stylesheet">


<div style="border:2px dashed gray; font-family:Gabarito">
    <%= image_tag wicked_pdf_asset_base64("NITK_Emblem.png"), style:'height:20px; width:20px' %
